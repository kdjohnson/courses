  "dependencies": {
    "@fullcalendar/daygrid": "^5.1.0",
    "@fullcalendar/list": "^5.1.0",
    "@fullcalendar/react": "^5.1.0",
    "@fullcalendar/timegrid": "^5.1.0",
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.0.0",
    "detect-browser": "^1.9.0",
    "iterators-polyfill": "^1.0.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-i18next": "^10.12.5",
    "react-redux": "^7.1.1",
    "reactjs-calendar": "^0.2.24",
    "redux": "^3.7.2",
    "redux-devtools": "^3.5.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "rewire": "^4.0.1",
    "typeface-arimo": "^0.0.22"
  },
  "devDependencies": {
    "react-scripts": "^3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "node scripts/build-non-split.js && npm run build:clean",
    "build:clean": "cd build/static/js/ && cp *.js ./main.js && cp *.map ./main.js.map && cp ../../service-worker.js .",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "npx prettier --write \"src/**/*.js\""
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
